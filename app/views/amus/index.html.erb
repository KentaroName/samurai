<h1>Listing amus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @amus.each do |amu| %>
      <tr>
        <td><%= amu.name %></td>
        <td><%= amu.age %></td>
        <td><%= link_to 'Show', amu %></td>
        <td><%= link_to 'Edit', edit_amu_path(amu) %></td>
        <td><%= link_to 'Destroy', amu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Amu', new_amu_path %>
